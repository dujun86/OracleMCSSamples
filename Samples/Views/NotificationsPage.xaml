<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Samples.Views.NotificationsPage">
    <StackLayout Orientation="Vertical" Padding="5, 25">
        <Button Text="Register for Notifications" 
			Clicked="Register"
			HorizontalOptions="Center" 
			WidthRequest="250"
            
			></Button>
        <Button Text="De-register for Notifications" 
			Clicked="Deregister"
			HorizontalOptions="Center" 
			WidthRequest="250"
			></Button>
    </StackLayout>
</ContentPage>